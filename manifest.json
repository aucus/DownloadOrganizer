{
    "manifest_version": 3,
    "name": "Download Organizer",
    "description": "Automatically organizes downloaded files into folders based on domain and file type.",
    "version": "1.0",
    "permissions": [
      "downloads",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "options_page": "options.html"
  }